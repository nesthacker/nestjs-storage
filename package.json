{
  "name": "@nesthacker/nestjs-storage",
  "version": "0.0.1",
  "description": "provides an injectable storage (S3, GS) client to provide storage access from NestJs modules",
  "author": "Sameer Khandke",
  "private": false,
  "license": "MIT",
  "repository": {
    "url": "git+https://github.com/coolsamk7/nestjs-storage.git"
  },
  "bugs": {
    "url": "https://github.com/coolsamk7/nestjs-storage/issues"
  },
  "keywords": [
    "nestjs-storage",
    "nestjs",
    "nestjs-s3",
    "nestjs-google-cloud-storage",
    "azure-storage"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json --runInBand",
    "test:e2e:dev": "jest --config ./tests/jest-e2e.json --runInBand --watch",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.10.0",
    "@google-cloud/storage": "^6.2.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.1.3",
    "@nestjs/core": "^7.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "@azure/storage-blob": "^12.10.0",
    "@commitlint/config-angular": "^17.0.3",
    "@google-cloud/storage": "^6.2.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "aws-sdk": "^2.1162.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "release-it": "^15.1.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "lib",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/coolsamk7/nestjs-storage#readme",
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
